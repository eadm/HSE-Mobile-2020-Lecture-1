<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.nobird.android.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".room.RoomActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DialogsActivity" />
        <activity android:name=".viewmodel.ViewModelActivity" />
        <activity android:name=".lifecycle.LifecycleExample" />
        <activity android:name=".FragmentsActivity" />
        <activity android:name=".fragment.arguments.ArgumentsActivity" />
        <activity android:name=".fragment.dynamic.FragmentDynamicActivity" />
        <activity android:name=".fragment.multiple.MultipleFragmentsActivity" />
        <activity android:name=".fragment.navigation.NavigationActivity" />
    </application>

</manifest>